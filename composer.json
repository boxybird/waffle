{
  "name": "boxybird/waffle",
  "description": "Tools built using Illuminate components (https://github.com/illuminate). Inspired by https://github.com/mattstauffer/Torch",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "illuminate",
    "laravel",
    "plugin"
  ],
  "authors": [
    {
      "name": "Andrew Rhyand",
      "email": "hello@andrewrhyand.com"
    }
  ],
  "autoload": {
