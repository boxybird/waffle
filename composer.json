    "illuminate/filesystem": "^12.2.0",
    "illuminate/console": "^12.2.0",
    "illuminate/encryption": "^12.2.0",
    "illuminate/events": "^12.2.0",
    "illuminate/queue": "^12.2.0",
    "illuminate/contracts": "^12.2.0",
    "illuminate/pagination": "^12.2.0",
    "illuminate/redis": "^12.2.0",
    "predis/predis": "^v2.3.0",
    "filp/whoops": "^2.18.0",
